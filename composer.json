{
  "name": "fitdev-pro/di",
  "description": "PHP 7.0 Dependency Container Implementation.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://fitdev.pro",
  "authors": [
    {
      "name": "Karol Kondraciuk",
      "email": "karol.kondraciuk@fitdev.pro",
      "role": "Developer"
    }
  ],
  "require": {
    "beberlei/assert": "^2.7",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "codacy/coverage": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "FitdevPro\\DI\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FitdevPro\\DI\\Tests\\": "tests/src",
      "FitdevPro\\DI\\TestsLib\\": "tests/lib"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
